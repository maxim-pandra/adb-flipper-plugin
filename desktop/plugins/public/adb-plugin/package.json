  },
  "devDependencies": {
    "flipper": "latest",
    "flipper-pkg": "latest"
  },
  "homepage": "https://github.com/maxim-pandra/adb-flipper-plugin#readme",
  "bugs": {
    "email": "turaevmaxim@gmail.com",
    "url": "https://github.com/maxim-pandra/adb-flipper-plugin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxim-pandra/adb-flipper-plugin"
  }
