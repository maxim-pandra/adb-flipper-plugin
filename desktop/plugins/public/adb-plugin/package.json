{
  "$schema": "https://fbflipper.com/schemas/plugin-package/v2.json",
  "name": "flipper-plugin-adb-plugin",
  "id": "adb-plugin",
  "pluginType": "device",
  "supportedDevices": [
    {
      "os": "Android",
      "archived": false
    }
  ],
  "version": "1.0.5",
  "description": "ADB helper for Flipper",
  "main": "dist/bundle.js",
  "flipperBundlerEntry": "src/index.tsx",
  "license": "MIT",
  "keywords": [
    "flipper-plugin"
  ],
  "icon": "tools",
